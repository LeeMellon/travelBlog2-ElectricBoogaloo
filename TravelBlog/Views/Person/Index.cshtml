@model IEnumerable<TravelBlog.Models.Person>
@{
    ViewData["Title"] = "Location - Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>@Html.DisplayNameFor(model => model.Name)</h4>
<ul>
    @foreach (var listPerson in Model)
    {
        <li>
            @Html.DisplayFor(model => listPerson.Name) |
            @Html.ActionLink("Details", "Details", new { id = listPerson.PersonId })
        </li>
    }
</ul>
